{
    "configurations": [
        {
            "name": "AVR",
            "intelliSenseMode": "clang-x64",
            "compilerPath": "/usr/bin/avr-gcc",
            "includePath": [
                "${workspaceFolder}",
                "${default}"
            ],
            "forcedInclude": [ "${workspaceFolder}/lib/ATmega_DFP/include/avr/iom324pb.h" ],
            "defines": ["__AVR_ATmega324PB__","F_CPU = 16000000UL"],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [ "${workspaceFolder}" ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}